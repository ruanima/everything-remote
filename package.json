{
  "name": "everything-remote-v3",
  "version": "0.1.0",
  "private": true,
  "main": "main/index.js",
  "homepage": ".",
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "electron-is-dev": "^1.1.0",
    "electron-preferences": "^1.3.2",
    "jsftp": "^2.1.3",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "react-virtualized": "^9.21.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && NODE_ENV=DEBUG electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.12",
    "@rescripts/rescript-env": "^0.0.10",
    "babel-cli": "^6.26.0",
    "concurrently": "^4.1.2",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "electron-reloader": "^0.3.0",
    "typescript": "^3.6.3",
    "wait-on": "^3.3.0"
  },
  "author": {
    "name": "Your Name",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "rescripts": [
    "env"
  ]
}
